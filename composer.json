{
    "name": "radbe/bot",
    "description": "vk callback bot",
    "type": "project",
    "require": {
        "php": "^7.2",
      "ext-curl": "*",
      "ext-json": "*",
      "ext-pdo": "*",
      "ext-mbstring": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "App/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
